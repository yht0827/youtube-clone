extends layouts/main
include mixins/videoBlock

block content  
    .search_header 
    .search__videos
    if videos.length === 0
      div.novideo
        img(src="http://fmda.in/images/found.gif")
        each item in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl,
                id:item.id
            })